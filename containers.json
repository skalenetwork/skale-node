{
  "schain": {
    "name": "skalenetwork/schain",
    "version": "1.43-develop.4",
    "config_options": {
      "rotateAfterBlock": 128
    },
    "custom_args": {
      "ulimits_list": [
        {
          "name": "core",
          "soft": -1,
          "hard": -1
        }
      ],
      "logs": {
        "max-size": "250m",
        "max-file": "5"
      }
    },
    "args": {
      "security_opt": [
        "seccomp=unconfined"
      ],
      "restart_policy": {
        "MaximumRetryCount": 10,
        "Name": "on-failure"
      },
      "network": "host",
      "cap_add": [
        "SYS_PTRACE"
      ]
    }
  },
  "ima": {
    "name": "skalenetwork/ima",
    "version": "1.0.0-develop.43",
    "custom_args": {},
    "args": {
      "restart_policy": {
        "MaximumRetryCount": 10,
        "Name": "on-failure"
      },
      "network": "host"
    }
  }
}
