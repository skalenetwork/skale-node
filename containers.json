{
  "schain": {
    "name": "skalelabshub/schain",
    "version": "1.0.0-develop.41",
    "custom_args": {
      "ulimits_list": [
        {
          "name": "core",
          "soft": -1,
          "hard": -1
        }
      ],
      "logs": {
        "max-size": "250m",
        "max-file": "5"
      }
    },
    "args": {
      "security_opt": [
        "seccomp=unconfined"
      ],
      "restart_policy": {
        "MaximumRetryCount": 10,
        "Name": "on-failure"
      },
      "network": "host"
    }
  },
  "ima": {
    "name": "skalelabshub/ima",
    "version": "1.0.0-develop.25",
    "custom_args": {},
    "args": {
      "restart_policy": {
        "MaximumRetryCount": 10,
        "Name": "on-failure"
      },
      "network": "host"
    }
  }
}
